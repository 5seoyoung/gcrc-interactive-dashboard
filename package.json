{
  "name": "gcrc-interactive-dashboard",
  "version": "1.0.0",
  "description": "Global Climate Risk Center - 실시간 기후 위험 데이터 플랫폼",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "client:dev": "vite",
    "client:build": "vite build",
    "server:dev": "cd server && nodemon index.js",
    "server:start": "cd server && node index.js",
    "blender:render": "cd blender && python climate_visualization.py",
    "test": "jest",
    "lint": "eslint src/",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.1.0",
    "mongoose": "^7.5.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "web3": "^4.1.1"
  },
  "devDependencies": {
    "@types/node": "^20.5.7",
    "concurrently": "^8.2.0",
    "cross-env": "^10.0.0",
    "eslint": "^8.48.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "vite": "^4.4.9"
  },
  "keywords": [
    "climate",
    "risk",
    "data-visualization",
    "blockchain",
    "real-time",
    "hackathon"
  ],
  "author": "HUSS Agora Hackathon Team",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  }
}
